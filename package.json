{
  "name": "lua-js",
  "version": "0.1.0",
  "description": "lua bindings for node",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Chris Bajorin <chris@chrisbajorin.com>",
  "license": "MIT",
  "bin": {
    "build-lua-js": "./build/index.js"
  },
  "scripts": {
    "test": "tsc -p tsconfig.json && node ./build/index.js && ava -v",
    "install": "build-lua-js --release"
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^12.19.4",
    "@types/rimraf": "^3.0.0",
    "ava": "^3.13.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "ava": {
    "files": [
      "tests/index.ts",
      "tests/versions/*.ts"
    ],
    "extensions": ["ts"],
    "require": ["ts-node/register"]
  }
}
